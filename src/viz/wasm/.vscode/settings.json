{
  "rust-analyzer.cargo.target": "wasm32-unknown-unknown",
  "rust-analyzer.server.extraEnv": {
    "CARGO_TARGET_DIR": "target/analyzer"
  },
  "rust-analyzer.check.extraArgs": ["--target-dir=target/analyzer"],
  "[rust]": {
    "editor.tabSize": 2
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Mozilla, ColumnLimit: 120, AlignAfterOpenBracket: BlockIndent, BinPackArguments: true, UseTab: Never, IndentWidth: 2, TabWidth: 1, BreakBeforeBraces: Attach, AllowShortIfStatementsOnASingleLine: true }",
  "files.associations": {
    "*.jsx": "javascript",
    "*.hbs": "html",
    "*.ipp": "cpp",
    "tuple": "cpp",
    "core": "cpp",
    "*.evaluator": "cpp",
    "alignedvector3": "cpp",
    "stdexcept": "cpp",
    "vector": "cpp",
    "__split_buffer": "cpp",
    "deque": "cpp",
    "list": "cpp",
    "string": "cpp",
    "hash_map": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "cstdint": "cpp",
    "map": "cpp",
    "unordered_map": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "string_view": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "numbers": "cpp",
    "ostream": "cpp",
    "ranges": "cpp",
    "streambuf": "cpp",
    "typeinfo": "cpp",
    "chrono": "cpp"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "geodesics/deps/**": true
  }
}
