{
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "src/viz/wasm/geodesics/deps/**": true
  },
  "files.associations": {
    "*.jsx": "javascript",
    "*.hbs": "html",
    "deque": "cpp",
    "list": "cpp",
    "vector": "cpp",
    "ranges": "cpp"
  },
  "eslint.probe": [
    "astro",
    "civet",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "mdx",
    "vue",
    "markdown",
    "json",
    "jsonc",
    "svelte"
  ],
  "eslint.validate": ["javascript", "javascriptreact", "svelte"]
}
